@using Microsoft.AspNetCore.Identity
@using ZyronChatWebApp.Models

@model ZyronChatWebApp.Models.UserScheduleListOfContacts



@{
    ViewData["Title"] = "Your list of contacts";
    ViewBag.ContactsInfoList = Model.ContactsInformations.ToArray();
}

@if(ViewBag.ContactsInfoList!=null)
{
    <h3 >Seus contatos</h3>
    <div class="list-group">

    @foreach (var contactinfo in ViewBag.ContactsInfoList)
    {
            
                <button type="button" class="list-group-item list-group-item-action list-group-item-dark"><h4>  @contactinfo.UsernameOfIdentification</h4></button>
    }

    </div>
}


<script src="~/js/signalr/dist/browser/signalr.js"></script>
<script src="~/js/chat.js"></script>
